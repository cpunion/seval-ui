{
	"name": "@seval-ui/seval",
	"version": "1.0.1",
	"description": "Seval - JavaScript-like language compiler for UI logic",
	"type": "module",
	"main": "./dist/seval.js",
	"module": "./dist/seval.js",
	"types": "./dist/seval.d.ts",
	"exports": {
		".": {
			"import": "./dist/seval.js",
			"types": "./dist/seval.d.ts"
		}
	},
	"files": ["dist"],
	"scripts": {
		"build": "tsc -p tsconfig.seval.json",
		"test": "bun test",
		"test:watch": "bun test --watch",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"coverage": "bun test --coverage",
		"prepublishOnly": "pnpm run build"
	},
	"keywords": ["seval", "compiler", "javascript", "dsl", "ui-logic"],
	"author": "cpunion",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/cpunion/seval-ui.git",
		"directory": "packages/seval"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/bun": "latest",
		"typescript": "^5.7.2"
	}
}
