{
	"name": "@seval-ui/seval",
	"version": "1.0.1",
	"description": "Seval compiler - JavaScript-like language that compiles to S-expressions",
	"type": "module",
	"main": "compiler.sexp",
	"types": "index.d.ts",
	"exports": {
		".": {
			"types": "./index.d.ts",
			"default": "./compiler.sexp"
		},
		"./source": {
			"types": "./index.d.ts",
			"default": "./compiler.sexp"
		},
		"./seval": {
			"types": "./seval.d.ts",
			"default": "./dist/seval.js"
		}
	},
	"files": ["compiler.sexp", "index.d.ts", "seval.d.ts", "dist"],
	"scripts": {
		"build": "tsc -p tsconfig.seval.json",
		"test": "bun test",
		"test:watch": "bun test --watch",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"coverage": "bun test --coverage",
		"prepublishOnly": "pnpm run build"
	},
	"keywords": ["seval", "s-expression", "self-hosted", "compiler"],
	"author": "cpunion",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/cpunion/seval-ui.git",
		"directory": "packages/seval"
	},
	"dependencies": {
		"@seval-ui/sexp": "workspace:*"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/bun": "latest",
		"typescript": "^5.7.2"
	}
}
