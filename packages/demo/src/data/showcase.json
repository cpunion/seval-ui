    "surfaceUpdate": {
      "surfaceId": "showcase",
      "components": [
        {
          "id": "code",
          "component": {
            "Code": {
              "lang": "seval",
              "code": "{\n  action_increment() {\n    [[\"counter\", counter + 1], [\"counterText\", \"Count: \" + (counter + 1)]]\n  },\n\n  action_decrement() {\n    [[\"counter\", counter - 1], [\"counterText\", \"Count: \" + (counter - 1)]]\n  },\n\n  action_reset() {\n    [[\"counter\", 0], [\"counterText\", \"Count: 0\"]]\n  },\n\n  action_selectItem() {\n    [[\"selectedItem\", get(context, \"name\")]]\n  },\n\n  action_addTodo() {\n    todoInput == \"\"\n      ? []\n      : [\n          [\"todos\", append(todos, obj(\"id\", nextTodoId, \"text\", todoInput, \"done\", false))],\n          [\"nextTodoId\", nextTodoId + 1],\n          [\"todoInput\", \"\"],\n          [\"todoStats\", length(filter((t) => get(t, \"done\"), append(todos, obj(\"id\", nextTodoId, \"text\", todoInput, \"done\", false)))) + \" of \" + (length(todos) + 1) + \" completed\"]\n        ]\n  },\n\n  action_deleteTodo() {\n    [\n      [\"todos\", filter((t) => get(t, \"id\") != get(context, \"id\"), todos)],\n      [\"todoStats\", length(filter((t) => get(t, \"done\"), filter((t) => get(t, \"id\") != get(context, \"id\"), todos))) + \" of \" + length(filter((t) => get(t, \"id\") != get(context, \"id\"), todos)) + \" completed\"]\n    ]\n  },\n\n  action_submitForm() {\n    formName == \"\" || formEmail == \"\"\n      ? [[\"formResultText\", \"Please fill in Name and Email\"]]\n      : [\n          [\"formSubmitted\", true],\n          [\"formResultText\", \"Submitted: \" + formName + \" <\" + formEmail + \">\" + (formMessage == \"\" ? \"\" : \" - \" + formMessage)]\n        ]\n  },\n\n  action_resetForm() {\n    [[\"formName\", \"\"], [\"formEmail\", \"\"], [\"formMessage\", \"\"], [\"formSubmitted\", false], [\"formResultText\", \"\"]]\n  },\n\n  updateDerived() {\n    [\n      [\"checkedText\", isChecked ? \"Checked!\" : \"Unchecked\"],\n      [\"todoStats\", length(filter((t) => get(t, \"done\"), todos)) + \" of \" + length(todos) + \" completed\"]\n    ]\n  }\n}"
            }
          }
        },
